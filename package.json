{
    "name": "lumerical-script-language",
    "displayName": "Lumerical Script Language",
    "description": "Syntax highlighting, code snippets, hover documentation and IntelliSense for Lumerical Script Language (.lsf files)",
    "publisher": "ErwenL",
    "version": "0.1.0",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ErwenL/lumerical-script-language.git"
    },
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "lumerical",
        "lsf",
        "ansys",
        "syntax-highlighting",
        "intellisense",
        "hover-documentation"
    ],
    "main": "./src/extension.js",
    "activationEvents": [
        "onLanguage:lumscript"
    ],
    "contributes": {
        "languages": [{
            "id": "lumscript",
            "aliases": ["Lumerical Script", "lumscript"],
            "extensions": [".lsf"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lumscript",
            "scopeName": "source.lumscript",
            "path": "./syntaxes/lumscript.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "lumscript",
                "path": "./snippets.json"
            }
        ]
    },
    "scripts": {
        "generate-commands": "node scripts/generate-commands.js",
        "test": "node test/command-loader.test.js",
        "package": "vsce package",
        "publish": "vsce publish"
    }
}