{
    "name": "lumerical-script-language",
    "displayName": "Lumerical Script Language",
    "description": "Syntax highlighting for lumerical script language",
    "publisher": "ErwenL",
    "version": "0.0.1",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ErwenL/lumerical-script-language.git"
    },
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "main": "./src/extension.js",
    "activationEvents": [
        "onLanguage:lumscript"
    ],
    "contributes": {
        "languages": [{
            "id": "lumscript",
            "aliases": ["Lumerical Script", "lumscript"],
            "extensions": [".lsf"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lumscript",
            "scopeName": "source.lumscript",
            "path": "./syntaxes/lumscript.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "lumscript",
                "path": "./snippets.json"
            }
        ]
    },
    "scripts": {
        "generate-commands": "node scripts/generate-commands.js",
        "test": "echo \"Error: no test specified\" && exit 1"
    }
}